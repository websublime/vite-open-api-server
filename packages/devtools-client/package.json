{
  "name": "@websublime/vite-plugin-open-api-devtools",
  "version": "0.8.4",
  "description": "DevTools SPA for vite-open-api-server - Vue-based debugging interface",
  "keywords": [
    "devtools",
    "vue",
    "openapi",
    "debugging",
    "spa"
  ],
  "license": "MIT",
  "author": "Websublime",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/devtools.umd.cjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/websublime/vite-open-api-server.git",
    "directory": "packages/devtools-client"
  },
  "scripts": {
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "vue-tsc --noEmit && vite build && pnpm run build:spa",
    "dev": "vite",
    "typecheck": "vue-tsc --noEmit",
    "build:spa": "vite build --config vite.config.spa.ts"
  },
  "dependencies": {
    "vue": "^3.5.17",
    "lucide-vue-next": "^0.513.0",
    "open-props": "^2.0.0-beta.5",
    "pinia": "^3.0.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "jsdom": "^26.1.0",
    "vite": "^7.0.0",
    "vue-tsc": "^2.2.12",
    "@vitejs/plugin-vue": "^6.0.0",
    "@ungap/structured-clone": "^1.3.0"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "private": false,
  "type": "module",
  "types": "./dist/devtools.d.ts",
  "sideEffects": [
    "./dist/style.css",
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/devtools.d.ts",
      "import": "./dist/devtools.js",
      "require": "./dist/devtools.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "module": "./dist/devtools.js"
}
