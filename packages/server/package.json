{
  "name": "@websublime/vite-plugin-open-api-server",
  "version": "0.24.0-next.8",
  "description": "Vite plugin for OpenAPI mock server with DevTools integration",
  "keywords": [
    "vite",
    "plugin",
    "openapi",
    "mock",
    "server",
    "devtools"
  ],
  "license": "MIT",
  "author": "Websublime",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/websublime/vite-open-api-server.git",
    "directory": "packages/server"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch",
    "copy:devtools-spa": "node scripts/copy-devtools-spa.mjs",
    "build": "tsup && pnpm run copy:devtools-spa"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "chokidar": "^5.0.0",
    "@websublime/vite-plugin-open-api-core": "workspace:*",
    "@vue/devtools-api": "^8.0.6",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@hono/node-server": "^1.14.3",
    "@types/node": "^20.17.10",
    "vite": "^7.0.0",
    "vue": "^3.5.27",
    "typescript": "^5.9.0",
    "hono": "^4.11.4",
    "@scalar/openapi-types": "^0.5.3",
    "@websublime/vite-plugin-open-api-devtools": "workspace:*",
    "@hono/node-ws": "^1.3.0",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "@hono/node-server": "^1.14.0",
    "@hono/node-ws": "^1.3.0",
    "hono": "^4.11.4",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@hono/node-ws": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "private": false,
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}